<Window x:Class="SFS.Search_Accounting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SFS"
        mc:Ignorable="d"
        Title="Search_Accounting" Height="350" Width="500">
    <Window.Background>
        <ImageBrush ImageSource="backgrd.png"/>
    </Window.Background>
    <Grid>
        <Label x:Name="label" Content="Enter ID:" HorizontalAlignment="Left" Margin="60,130,0,0" VerticalAlignment="Top" FontSize="22" FontFamily="Traditional Arabic" FontWeight="Bold"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="213,130,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="184" TextChanged="textBox_TextChanged"/>
        <Button x:Name="button" Content="" HorizontalAlignment="Left" Margin="367,241,0,0" VerticalAlignment="Top" Width="100" Height="70">
            <Button.Background>
                <ImageBrush ImageSource="search.png"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
